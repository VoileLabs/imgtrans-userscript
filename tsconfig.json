{
  "compilerOptions": {
    "target": "ES2018",
    "useDefineForClassFields": true,
    "module": "ESNext",
    "lib": ["ESNext", "DOM"],
    "types": ["tampermonkey", "vitest/globals"],
    "moduleResolution": "Node",
    "strict": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "noEmit": true,
    "noImplicitReturns": true
  },
  "include": ["./src"]
}
